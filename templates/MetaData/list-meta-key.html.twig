{% extends get_admin_template('base_list_field', admin.code) %}

{% block field %}
    {# @var object \App\Entity\MetaData\AbstractMetaItem #}
    {%- apply spaceless %}
        {%- if object.internalLabel is not empty -%}
            {{ object.internalLabel|trans }}
        {%- elseif value is empty -%}
            &nbsp;
        {%- else -%}
            {{ value|trans }}
        {%- endif -%}
    {% endapply -%}
{% endblock %}
