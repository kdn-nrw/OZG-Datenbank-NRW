<div class="box box-primary">
    <div class="box-header">
        <h4 class="box-title">
            {{ 'app.inquiry.inquiry_about'|trans }} {{ app_get_entity_label(referenceSource)|trans }}
            {%- if object is defined and object is not null -%}
                : {{ object ~ '' }}
            {%- endif -%}
        </h4>
    </div>
    <div class="box-body no-padding">
        <div class="inquiry-list-body box-comments">
            {% for inquiry in inquiries %}
                {% include 'Onboarding/Inquiry/Partials/_list-item.html.twig' with {'showAnswers': true} %}
            {% endfor %}
        </div>
    </div>
    <div class="box-footer">
        <div class="inquiry-list-footer">
            {% if backUrl %}
                <a href="{{ backUrl }}" class="btn btn-default" title="{{ 'app.common.back_to_list'|trans }}">
                    <i class="fa fa-chevron-left" aria-hidden="true"></i>
                    {{ 'app.common.back'|trans }}
                </a>
            {% endif %}
        </div>
    </div>
</div>