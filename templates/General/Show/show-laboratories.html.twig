{% if object.laboratories|length > 0 %}
    {# @var field_description \Sonata\DoctrineORMAdminBundle\Admin\FieldDescription #}
    {% set route_name = field_description.options.route.name %}
    {% set createEditLink = route_name and field_description.hasassociationadmin
        and field_description.associationadmin.hasRoute(route_name) and field_description.associationadmin.hasAccess(route_name) %}
    <div class="box box-primary">
        <div class="box-header">
            <h4 class="box-title">
                {{ 'app.laboratory.list'|trans }}
            </h4>
        </div>
        <div class="box-body">
            <table class="table table-bordered table-condensed">
                <tr>
                    <th>
                        {{ 'app.laboratory.entity.name'|trans }}
                    </th>
                    <th>
                        {{ 'app.laboratory.entity.url'|trans }}
                    </th>
                    <th>
                        {{ 'app.laboratory.entity.description'|trans }}
                    </th>
                    <th>
                        {{ 'app.laboratory.entity.service_providers'|trans }}
                    </th>
                    <th>
                        {{ 'app.laboratory.entity.participants_other'|trans }}
                    </th>
                    {% if createEditLink %}
                        <th></th>
                    {% endif %}
                </tr>
                {% for child in object.laboratories %}
                    {# @var child \App\Entity\Laboratory #}
                    <tr>
                        <td>
                            {{ child.name }}
                        </td>
                        <td>
                            {% if child.url %}
                                {{ child.url }}
                            {% endif %}
                        </td>
                        <td>
                            {{ child.description|nl2br }}
                        </td>
                        <td>
                            {% if child.serviceProviders %}
                                {% for grandChild in child.serviceProviders %}
                                    <span class="badge">{{ grandChild.name }}</span>
                                {% endfor %}
                            {% endif %}
                        </td>
                        <td>
                            {% if child.participantsOther %}
                                {{ child.participantsOther }}
                            {% endif %}
                        </td>
                        {% if createEditLink %}
                        <td>
                            {% if field_description.associationadmin.hasAccess(route_name, child) %}
                                <a href="{{ field_description.associationadmin.generateObjectUrl(route_name, child, field_description.options.route.parameters) }}">
                                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                </a>
                            {% endif %}
                        </td>
                        {% endif %}
                    </tr>
                {% endfor %}
            </table>
        </div>
    </div>
{% endif %}