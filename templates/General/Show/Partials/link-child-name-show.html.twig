{# @var refSettings \App\Model\ReferenceSettings #}
{% if refSettings.show.enabled and refSettings.admin.hasAccess(refSettings.show.route, child) %}
    <a href="{{ refSettings.admin.generateObjectUrl(refSettings.show.route, child, {'id': child.id}) }}">
        {{ child.name }}
    </a>
{% else %}
    {{ child.name }}
{% endif %}