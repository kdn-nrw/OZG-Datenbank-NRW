{# @var refSettings \App\Model\ReferenceSettings #}
<table class="table table-bordered table-striped table-condensed">
    <tr>
        {% set tableHeaders = ['name', 'street', 'zip_code', 'url', 'commune_type', 'constituency', 'main_email', 'official_community_key', 'administrative_district'] %}
        {% include 'General/Show/Table/table-head-general.html.twig' with {'tableHeaders': tableHeaders, 'labelPrefix': refSettings.labelPrefix} %}
        {% if refSettings.edit.enabled %}
            <th></th>
        {% endif %}
    </tr>
    {% for child in entities %}
        {# @var child \App\Entity\StateGroup\Commune #}
        {% include 'General/Show/Table/table-row-cells-communes.html.twig' with {'child': child} %}
    {% endfor %}
</table>