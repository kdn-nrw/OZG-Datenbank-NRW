{# @var object \App\Entity\AbstractService #}
{% if elements['jurisdictions'] is defined and object.jurisdictions|length > 0 %}
    <div class="col-xs-12">
        <div class="col-xs-12 col-md-2">
            {{ elements['jurisdictions']|render_view_element(object)}}
        </div>
        {% if object.stateMinistriesEnabled and object.stateMinistries|length > 0 and elements['stateMinistries'] is defined %}
            <div class="col-xs-12 col-md-5 col-lg-4 col-xl-3">
                {{ elements['stateMinistries']|render_view_element(object)}}
            </div>
        {% endif %}
        {% if elements['bureaus'] is defined and object.bureausEnabled and object.bureaus|length > 0 %}
            <div class="col-xs-12 col-md-5 col-lg-4 col-xl-3">
                {{ elements['bureaus']|render_view_element(object)}}
            </div>
        {% endif %}
    </div>
{% endif %}