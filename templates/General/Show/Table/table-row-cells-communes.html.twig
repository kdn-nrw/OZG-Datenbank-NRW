{# @var child \App\Entity\StateGroup\Commune #}
<tr>
    <td>
        {% include 'General/Show/Partials/link-child-name-show.html.twig' %}
    </td>
    <td>
        {% if child.street %}
            {{ child.street }}
        {% endif %}
    </td>
    <td>
        {% if child.zipCode %}
            {{ child.zipCode }}
        {% endif %}
        {% if child.town %}
            {{ child.town }}
        {% endif %}
    </td>
    <td>
        {% if child.url %}
            {{ child.url }}
        {% endif %}
    </td>
    <td>
        {% if child.communeType %}
            {{ child.communeType.name }}
        {% endif %}
    </td>
    <td>
        {% if child.constituency %}
            {{ child.constituency.name }}
        {% endif %}
    </td>
    <td>
        {% if child.mainEmail %}
            {{ child.mainEmail }}
        {% endif %}
    </td>
    <td>
        {% if child.officialCommunityKey %}
            {{ child.officialCommunityKey }}
        {% endif %}
    </td>
    <td>
        {% if child.administrativeDistrict %}
            {{ child.administrativeDistrict.name }}
        {% endif %}
    </td>
    {% if refSettings.edit.enabled %}
        <td>
            {% include 'General/Show/Partials/link-to-child.html.twig' %}
        </td>
    {% endif %}
</tr>