{
    "name": "kdn-nrw/ozg",
    "description" : "The data warehouse application from KDN NRW containing the OZG database",
    "type": "project",
    "license": "MIT",
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.7",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13.3",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "jms/serializer-bundle": "^5.0.2",
        "lexik/jwt-authentication-bundle": "^2.16.0",
        "liip/imagine-bundle": "^2.7",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.25.2",
        "setasign/fpdi": "^v2.3.6",
        "shapecode/cron-bundle": "~5.0 || dev-master",
        "sonata-project/admin-bundle": "^4.21.0",
        "sonata-project/doctrine-orm-admin-bundle": "5.x-dev",
        "sonata-project/entity-audit-bundle": "1.8.0",
        "sonata-project/formatter-bundle": "5.0.1",
        "sonata-project/intl-bundle": "^2.4.13 || ^3.0.2",
        "sonata-project/translation-bundle": "^3.1.0",
        "sonata-project/user-bundle": "5.4.0",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^v1.0.1",
        "symfony/asset": "~4.4 || ^5.0 || ^6.0",
        "symfony/dotenv": "~4.4 || ^5.0 || ^6.0",
        "symfony/expression-language": "~4.4 || ^5.0 || ^6.0",
        "symfony/flex": "^1.19.3|^2.1.8",
        "symfony/http-client": "~4.4 || ^5.0 || ^6.0",
        "symfony/mailer": "~4.4 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.8.0",
        "symfony/property-access": "~4.4 || ^5.0 || ^6.0",
        "symfony/property-info": "~4.4 || ^5.0 || ^6.0",
        "symfony/proxy-manager-bridge": "~4.4 || ^5.0 || ^6.0",
        "symfony/runtime": "^5.0 || ^6.0",
        "symfony/security-bundle": "~4.4 || ^5.0 || ^6.0",
        "symfony/security-http": "~4.4 || ^5.0 || ^6.0",
        "symfony/serializer": "~4.4 || ^5.0 || ^6.0",
        "symfony/twig-bundle": "~4.4 || ^5.0 || ^6.0",
        "symfony/validator": "~4.4 || ^5.0 || ^6.0",
        "symfony/webpack-encore-bundle": "^1.16.0",
        "symfony/yaml": "~4.4 || ^5.0 || ^6.0",
        "tecnickcom/tcpdf": "^6.5.0",
        "twig/cache-extra": "^3.4.0",
        "twig/extra-bundle": "^3.4.0",
        "twig/intl-extra": "^3.4.2",
        "vich/uploader-bundle": "^1.21.1 || ^2.0.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7.5",
        "symfony/browser-kit": "~4.4 || ^5.0 || ^6.0",
        "symfony/css-selector": "~4.4 || ^5.0 || ^6.0",
        "symfony/debug-bundle": "~4.4 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "~4.4 || ^5.0 || ^6.0",
        "symfony/stopwatch": "~4.4 || ^5.0 || ^6.0",
        "symfony/web-profiler-bundle": "~4.4 || ^5.0 || ^6.0"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop --exclude=samples": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "bin/phpunit ./tests/"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
