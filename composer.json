{
    "type": "project",
    "license": "proprietary",
    "repositories": [
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:mindbase/ansible.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:tools/docker.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/admin-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/email-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/entity-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/form-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/log-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/settings-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/symfony-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/user-bundle.git" },
        { "type": "vcs", "url": "git@gitlab.brain-appeal.net:symfony/mindbase/vuejs-bundle.git" }
    ],
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "mindbase/admin-bundle": "dev-develop",
        "mindbase/email-bundle": "dev-develop",
        "mindbase/entity-bundle": "dev-develop",
        "mindbase/form-bundle": "dev-develop",
        "mindbase/log-bundle": "dev-develop",
        "mindbase/settings-bundle": "dev-develop",
        "mindbase/symfony-bundle": "dev-develop",
        "mindbase/user-bundle": "dev-develop",
        "mindbase/vuejs-bundle": "dev-develop",
        "shapecode/cron-bundle": "^3.2",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/serializer": "4.3.*",
        "symfony/yaml": "4.3.*",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "brainappeal/ansible": "dev-master|dev-develop",
        "brainappeal/docker": "^0.3.8|dev-develop",
        "oomphinc/composer-installers-extender": "^1.1",
        "symfony/browser-kit": "4.3.*",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        },
        "installer-types": ["brainappeal-dev"],
        "installer-paths": {
            "doc-dev/{$name}/": ["type:brainappeal-dev"]
        }
    }
}
